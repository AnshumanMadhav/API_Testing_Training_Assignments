package Assignment1;

public class Problem13
{
    public static void main(String[] args)
    {
      String str1 = "Welcome to Java";
      System.out.println("Original String is: "+str1);
      Problem13 pb13 = new Problem13();
      pb13.reverse_string(str1);
    }
    public void reverse_string(String str)
    {

        String rev_str1 = "";
        for(int i = 0;i<str.length();i++)
        {
            char ch = str.charAt(i);
            rev_str1 = ch+rev_str1;
        }
        System.out.println("Reverse String is: "+rev_str1);
    }
}
